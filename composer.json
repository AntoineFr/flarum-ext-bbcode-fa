{
    "name": "antoinefr/flarum-ext-bbcode-fa",
    "description": "Insert FontAwesome icons in your posts.",
    "type": "flarum-extension",
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "autoload": {
        "psr-4": {
            "AntoineFr\\BBCode\\FA\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "BBCode FA",
            "icon": {
                "name": "fab fa-font-awesome",
                "backgroundColor": "#339AF0",
                "color": "#FFFFFF"
            }
        }
    }
}